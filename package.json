  "bugs": {
    "url": "https://github.com/Banno/web-component-router/issues"
  },
  "homepage": "https://github.com/Banno/web-component-router",
  "dependencies": {
    "path-to-regexp": "^6.2.1"
  },
  "resolutions": {
    "@types/node": "^18.11.18"
  },
  "devDependencies": {
    "@polymer/polymer": "^3.4.1",
    "@types/node": "^22.10.0",
    "@vitest/browser": "^2.1.6",
    "playwright": "1.49.0",
    "rimraf": "^6.0.1",
    "typescript": "^5.2.2",
    "vite": "6.0.0",
    "vitest": "^2.1.6"
  }
